          "comparator": "<",
          "value": <%= threshold_value('((avg:bosh.healthmonitor.system.healthy{deployment:,job:runner_z1}.rollup(avg,60)) +(avg:bosh.healthmonitor.system.healthy{deployment:,job:runner_z2}.rollup(avg,60)) ) * 0.5', 'white_on_red') %>
