      "width": 66,
      "timeframe": "4h",
      "y": 4,
      "x": 66,
      "type": "timeseries"
    },
    {
      "title_size": "16",
      "title": true,
      "title_align": "left",
      "title_text": "User CPU",
      "height": 21,
      "tile_def": {
        "viz": "timeseries",
